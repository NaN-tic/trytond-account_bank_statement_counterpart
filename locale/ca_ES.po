msgctxt "error:account.bank.statement.line:"
msgid ""
"Cannot create counterpart with same account \"%(account)s\", check line "
"\"%(line)s\" and journal \"%(journal)s\"."
msgstr ""
"No es pot crear una contrapartida amb el mateix compte \"%(account)s\", "
"reviseu la línia \"%(line)s\" i diari \"%(journal)s\"."

msgctxt "error:account.bank.statement.line:"
msgid "Journal \"%s\" has not got credit or debit account configured."
msgstr "El diari \"%s\" no té el compte d'haver o deure configurat."

msgctxt "error:account.bank.statement.line:"
msgid ""
"The credit or debit account of Journal \"%s\" is not checked as \"Bank "
"Conciliation\"."
msgstr ""
"El compte d'haver o deure del diari \"%s\" no està marcat com a "
"\"Conciliació bancària\"."

msgctxt "error:account.move.reconciliation:"
msgid ""
"Cannot break reconciliation \"%(reconciliation)s\" because line \"%(line)s\""
" has assigned to Bank Statement Line Please go to \"%(statement_line)s\" "
"bank statement line to unreconcile"
msgstr ""
"No es pot trencar la conciliació \"%(conciliation)s\" perquè la línia "
"\"%(line)s\" està assignada a la línia d'extracte bancari. Aneu a la línia "
"de l'extracte \"%(statement_line)s\" i trenqueu la conciliació."

msgctxt "field:account.bank.statement.line,account_date:"
msgid "Account Date"
msgstr "Data de comptabilització"

msgctxt "field:account.bank.statement.line,counterpart_lines:"
msgid "Counterpart"
msgstr "Contrapartida"

msgctxt "field:account.move.line,bank_statement_line_counterpart:"
msgid "Bank Statement Line Counterpart"
msgstr "Contrapartida de l'extracte bancari"
