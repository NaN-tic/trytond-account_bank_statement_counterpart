msgctxt "error:account.bank.statement.line:"
msgid ""
"Cannot create counterpart with same account \"%(account)s\", check line "
"\"%(line)s\" and journal \"%(journal)s\"."
msgstr ""
"No se puede crear una contrapartida con la misma cuenta \"%(account)s\", "
"revise la línea \"%(line)s\" y el diario \"%(journal)s\"."

msgctxt "error:account.bank.statement.line:"
msgid "Journal \"%s\" has not got credit or debit account configured."
msgstr "El diario \"%s\" no tiene la cuenta haber o debe configurada."

msgctxt "error:account.bank.statement.line:"
msgid ""
"The credit or debit account of Journal \"%s\" is not checked as \"Bank "
"Conciliation\"."
msgstr ""
"La cuenta haber o debe del diario \"%s\" no está marcada como \"Conciliación"
" bancaria\"."

msgctxt "error:account.move.reconciliation:"
msgid ""
"Cannot break reconciliation \"%(reconciliation)s\" because line \"%(line)s\""
" has assigned to Bank Statement Line Please go to \"%(statement_line)s\" "
"bank statement line to unreconcile"
msgstr ""
"No se puede romper la conciliación \"%(reconciliation)s\" porqué la línea "
"\"%(line)s\"està asignada en  la línea del extracto bancario. Vaya a la "
"línea \"%(statement_line)s\" y rompa la conciliación."

msgctxt "field:account.bank.statement.line,account_date:"
msgid "Account Date"
msgstr "Fecha de contabilización"

msgctxt "field:account.bank.statement.line,account_date_utc:"
msgid "Account Date UTC"
msgstr "Fecha de contabilización UTC"

msgctxt "field:account.bank.statement.line,counterpart_lines:"
